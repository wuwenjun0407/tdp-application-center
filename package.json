{
    "name": "tdp-application-center",
    "private": true,
    "version": "0.0.0",
    "scripts": {
        "dev": "vite",
        "serve": "pnpm dev",
        "build": "rimraf dist && vue-tsc --noEmit && vite build",
        "preview": "vite preview",
        "clean:cache": "rimraf node_modules && rimraf .eslintcache && pnpm install",
        "lint": "pnpm lint:eslint && pnpm lint:stylelint & pnpm lint:prettier",
        "lint:prettier": "prettier --write  \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
        "lint:eslint": "eslint --cache --max-warnings 0  \"src/**/*/*.{vue,ts,tsx}\" --fix",
        "lint:stylelint": "stylelint --cache --fix \"**/*.{vue,css,scss,postcss,less}\" --cache --cache-location node_modules/.cache/stylelint/",
        "preinstall": "npx only-allow pnpm",
        "prepare": "husky install"
    },
    "dependencies": {
        "@types/node": "^17.0.21",
        "axios": "^0.26.0",
        "dayjs": "^1.10.8",
        "echarts": "^5.3.0",
        "element-plus": "^2.0.4",
        "lodash-es": "^4.17.21",
        "nprogress": "^0.2.0",
        "path": "^0.12.7",
        "pinia": "^2.0.0-rc.10",
        "responsive-storage": "^1.0.11",
        "vue": "^3.2.25",
        "vue-i18n": "^9.1.9",
        "vue-router": "^4.0.13",
        "vue-types": "^4.1.1"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^5.13.0",
        "@typescript-eslint/parser": "^5.13.0",
        "@vitejs/plugin-vue": "^2.2.0",
        "@vue/eslint-config-prettier": "^7.0.0",
        "@vue/eslint-config-typescript": "^10.0.0",
        "eslint": "^8.10.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-vue": "^8.5.0",
        "husky": "^7.0.4",
        "postcss-html": "^1.3.0",
        "prettier": "^2.5.1",
        "rimraf": "^3.0.2",
        "sass": "^1.49.9",
        "stylelint": "^14.5.3",
        "stylelint-config-html": "^1.0.0",
        "stylelint-config-prettier": "^9.0.3",
        "stylelint-config-recommended": "^7.0.0",
        "stylelint-config-standard": "^25.0.0",
        "stylelint-order": "^5.0.0",
        "typescript": "^4.5.4",
        "vite": "^2.8.0",
        "vue-tsc": "^0.29.8"
    }
}
